datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}

model Visit {
  id   Int      @id @default(autoincrement())
  time DateTime @default(now())
  // name      String?  @default("anonymous")

  @@map("visits")
}
